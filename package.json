{
    "name": "Power-Ai",
    "version": "3.6.1",
    "author": {
        "name": "宁波轻蜓视觉科技有限公司",
        "url": "https://github.com/yiningzeng/Power-Ai.git"
    },
    "description": "Power Ai (PA) - an annotation and labeling tool for images and video.",
    "homepage": "https://github.com/yiningzeng/Power-Ai.git",
    "repository": {
        "type": "git",
        "url": "https://github.com/yiningzeng/Power-Ai.git"
    },
    "license": "MIT",
    "private": true,
    "main": "build/electron.js",
    "dependencies": {
        "@azure/storage-blob": "^10.3.0",
        "@material-ui/core": "^4.5.1",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/styles": "^4.5.0",
        "@tensorflow/tfjs": "^1.0.3",
        "@types/snapsvg": "^0.4.35",
        "@types/webgl2": "0.0.5",
        "amqplib": "^0.5.5",
        "archiver": "^3.1.1",
        "axios": "^0.18.0",
        "babel-plugin-dynamic-import-node": "^2.3.0",
        "bootstrap": "^4.1.3",
        "bound-points": "^1.0.0",
        "buffer-reverse": "^1.0.1",
        "child_process": "^1.0.2",
        "chokidar": "^3.3.0",
        "clsx": "^1.0.4",
        "crypto-js": "^3.1.9-1",
        "dotenv": "^7.0.0",
        "electron-log": "^3.0.8",
        "electron-packager": "^14.1.0",
        "electron-updater": "^4.2.0",
        "ftp": "^0.3.10",
        "google-protobuf": "^3.6.1",
        "got": "^9.6.0",
        "hddserial": "0.0.7",
        "hexrgb": "0.0.2",
        "jpeg-js": "^0.3.4",
        "json2yaml": "^1.1.0",
        "lodash": "^4.17.11",
        "mariadb": "^2.1.4",
        "md5.js": "^1.3.5",
        "moment": "^2.24.0",
        "node-fetch": "^2.3.0",
        "node-int64": "^0.4.0",
        "omnizoom": "^1.1.5",
        "polygon-aabb": "^1.0.0",
        "powerai-ct": "^2.3.1",
        "rc-align": "^2.4.5",
        "rc-checkbox": "^2.1.6",
        "rc-menu": "^7.4.21",
        "rc-slider": "^8.6.7",
        "rc-tree-select": "^2.7.1",
        "react": "^16.8.6",
        "react-appinsights": "^3.0.0-rc.5",
        "react-color": "^2.17.3",
        "react-dnd": "^5.0.0",
        "react-dnd-html5-backend": "^3.0.2",
        "react-dom": "^16.11.0",
        "react-iframe": "^1.8.0",
        "react-jsonschema-form": "^2.0.0-alpha.1",
        "react-localization": "^1.0.13",
        "react-modal": "^3.8.1",
        "react-redux": "^5.1.1",
        "react-rnd": "^9.1.2",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.1",
        "react-split-pane": "^0.1.87",
        "react-tag-input": "^6.1.0",
        "react-toastify": "^4.5.2",
        "react-virtualized": "^9.21.0",
        "react-vis": "^1.11.6",
        "reactstrap": "^6.5.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "rimraf": "^2.6.2",
        "rotating-calipers": "0.0.0",
        "sequelize": "^5.21.2",
        "shortid": "^2.2.14",
        "tar": "^4.4.10",
        "typescript": "^3.7.1-rc",
        "update-electron-app": "^1.5.0",
        "video-react": "^0.14.1",
        "vott-react": "^0.2.10"
    },
    "scripts": {
        "start": "nf start -p 3000",
        "build-first": "npm run build && npm run webpack:prod",
        "build-win32-nsis": "electron-builder --win --ia32 nsis",
        "build-win64-nsis": "electron-builder --win --x64 nsis",
        "build-mac-dmg": "electron-builder --mac zip",
        "build-linux-deb-snap-tar": "electron-builder --linux snap deb tar.xz",
        "compile": "tsc",
        "build": "react-scripts build",
        "webpack:dev": "webpack --config ./config/webpack.dev.js",
        "webpack:prod": "webpack -p --config ./config/webpack.prod.js",
        "react-start": "react-scripts start",
        "electron:run:dev": "npm run webpack:dev && electron . --remote-debugging-port=9223",
        "electron:run:prod": "npm run webpack:prod && electron . --remote-debugging-port=9223",
        "electron:start:dev": "npm run webpack:dev && npm run electron-start",
        "electron:start:prod": "npm run webpack:prod && npm run electron-start",
        "electron-start": "node src/electron/start",
        "eject": "react-scripts eject",
        "release-pr": "./scripts/release-pr.sh",
        "release-web": "npm run build && npm run webpack:prod",
        "release-ci": "bash ./scripts/build.sh",
        "release": "npm run build && npm run webpack:prod && electron-builder",
        "pretest": "./node_modules/.bin/tslint 'src/**/*.ts*'",
        "lintfix": "./node_modules/.bin/tslint 'src/**/*.ts*' --fix",
        "test": "react-scripts test --env=jsdom --silent",
        "test:ci": "cross-env CI=true npm run test",
        "test:coverage": "npm run test -- --coverage",
        "plato": "scripts/generate-report.sh -o report -v $(node -pe \"require('./package.json').version\") -c $(git rev-parse --short HEAD)",
        "postinstall": "electron-builder install-app-deps",
        "predebug": "npm run build && npm run webpack:dev"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.11.2",
        "@types/axios": "^0.14.0",
        "@types/dotenv": "^6.1.0",
        "@types/enzyme": "^3.1.15",
        "@types/jest": "23.3.9",
        "@types/node": "12.11.7",
        "@types/react": "16.9.11",
        "@types/react-dom": "16.9.3",
        "@types/react-jsonschema-form": "^1.6.5",
        "@types/react-router-dom": "^5.1.0",
        "@types/react-split-pane": "^0.1.67",
        "@types/react-toastify": "^4.0.1",
        "@types/reactstrap": "^8.0.6",
        "@types/redux-logger": "^3.0.6",
        "@types/redux-mock-store": "^1.0.0",
        "cross-env": "^5.2.0",
        "electron": "^11.5.0",
        "electron-builder": "^21.2.0",
        "electron-rebuild": "^1.7.3",
        "electron-wix-msi": "^2.1.1",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.7.0",
        "foreman": "^3.0.1",
        "jest-enzyme": "^7.0.1",
        "jquery": "^3.3.1",
        "node-sass": "^4.13.0",
        "popper.js": "^1.14.6",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "ts-loader": "^5.3.0",
        "tslint": "^5.11.0",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.5"
    },
    "engines": {
        "node": ">=13.0.1",
        "npm": ">=6.12.0"
    },
    "build": {
        "appId": "com.yiningzeng.power-ai",
        "directories": {
            "output": "releases",
            "buildResources": "app-icons"
        },
        "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
        "files": [
            "**/*",
            "**/releases/*",
            "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
            "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
            "!**/node_modules/*.d.ts",
            "!**/node_modules/.bin",
            "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
            "!.editorconfig",
            "!**/._*",
            "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
            "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
            "!**/{appveyor.yml,.travis.yml,circle.yml}",
            "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
        ],
        "extraFiles": "cocoSSDModel",
        "publish": [
            {
                "provider": "generic",
                "url": "http://updater.qtingvision.com:888"
            }
        ],
        "nsis": {
            "artifactName": "${productName}-${os}-${arch}-${version}-setup.${ext}",
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "app-icons/ico.ico",
            "uninstallerIcon": "app-icons/ico.ico"
        },
        "win": {
            "target": "nsis",
            "legalTrademarks": "宁波轻蜓科技技术有限公司"
        },
        "mac": {
            "icon": "app-icons/icon.icns"
        },
        "linux": {
            "target": "snap",
            "icon": "app-icons/ico.png",
            "maintainer": "yiningzeng",
            "vendor": "宁波轻蜓科技技术有限公司",
            "category": "Development",
            "desktop": "Power Ai"
        }
    }
}
